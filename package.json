{
  "name": "bubble-ai",
  "version": "1.0.0",
  "private": true,
  "packageManager": "npm@10.9.2",
  "description": "AI/ML utilities for Bubble Goods - taxonomy embeddings, categorization, and more",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo build",
    "test": "turbo test",
    "test:run": "turbo test:run",
    "typecheck": "turbo typecheck",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "populate-embeddings": "tsx scripts/populate-embeddings.ts",
    "api:dev": "npm run dev -w @bubble-ai/api",
    "api:wrangler": "npm run wrangler -w @bubble-ai/api",
    "api:deploy": "npm run deploy -w @bubble-ai/api",
    "api:deploy:staging": "npm run deploy:staging -w @bubble-ai/api"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bubble-goods/bubble-ai.git"
  },
  "keywords": [
    "shopify",
    "taxonomy",
    "embeddings",
    "ai",
    "categorization"
  ],
  "author": "Bubble Goods",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bubble-goods/bubble-ai/issues"
  },
  "homepage": "https://github.com/bubble-goods/bubble-ai#readme",
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "dotenv": "^17.2.3",
    "tsx": "^4.21.0",
    "turbo": "^2.5.3"
  }
}
